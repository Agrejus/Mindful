  "scripts": {
    "build-electron:dev": "npm run clean && webpack --config electron/config/dev/webpack.electron.config.dev.ts",
    "web-app": "npm run start",
    "start-web": "webpack --config config/dev/webpack.config.dev.ts && webpack-dev-server --config config/dev/webpack.config.dev.ts --colors --inline",
    "start:stage": "npm run clean && webpack --config config/stage/webpack.config.stage.ts && webpack-dev-server --config config/stage/webpack.config.stage.ts --colors --inline",
    "start:prod": "npm run clean && webpack --config config/prod/webpack.config.prod.ts && webpack-dev-server --config config/prod/webpack.config.prod.ts --colors --inline",
    "build:dev": "npm run clean && webpack --config config/dev/webpack.config.dev.ts",
    "build:stage": "npm run clean && webpack --config config/stage/webpack.config.stage.ts",
    "build": "./node_modules/.bin/rimraf dist/* && webpack --config config/prod/webpack.config.prod.ts",
    "clean": "npm run clean-dist && npm run clean-build",
    "clean-dist": "./node_modules/.bin/rimraf dist/*",
    "clean-build": "./node_modules/.bin/rimraf build/*",
    "electron": "electron ./dist/app-launcher.js",
    "start": "npm run clean && webpack --config electron/config/dev/webpack.electron.config.dev.ts && npm run build-server && node ./dist/app-start.js",
    "tsc": "tsc --p ./electron/ --project ./electron/tsconfig.json",
    "express": "node ./dist/server.js",
    "watch": "webpack --watch --config config/dev/webpack.config.dev.ts",
    "lint": "eslint . --ext .ts",
    "build-server": "webpack --config server/config/dev/webpack.server.config.dev.ts"
  },
